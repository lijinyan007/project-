@import "../common/mainTabCell/mainTabCell.wxss";
page{
  background: #f6f6f6;
}
swiper image{
  width: 100%;
  height: 100%;
  display: block;
}
.mainTab{
  height: 75rpx;
  width: 100%;
  display: flex;
  background:#fff;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 8;
}
.tabItem{
  flex: 1;
  text-align: center;
  height: 75rpx;
  line-height: 75rpx;
  font-size: 26rpx;
}
.tabItem.active{
  color:#d4237a;
  font-weight: bold;
}