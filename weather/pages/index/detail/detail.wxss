
.detailcontainer{
  width: 100%;
  color: #666;
  font-size: 30rpx;
}
.detailcontainer .detail_item,.detailcontainer .time{
  height: 100rpx;
  padding:0 40rpx;
  line-height: 100rpx;
}
.detailcontainer .detail_item image{
  width: 90rpx;
  height: 90rpx;
  display: inline-block;
  padding-top: 5rpx;
  padding-left: 5rpx;
  vertical-align: top;
}
.detailcontainer .detail_item text{
  height: 100rpx;
  line-height: 100rpx;
  vertical-align: top;
}
.detailcontainer .dressing_advice{
  height: 150rpx;
  line-height: 30rpx;
  position: relative
}
.detailcontainer .dressing_advice text{
   line-height: 40rpx;
   position: absolute;
   left: 135rpx;
   top: 10rpx;
}
.detailcontainer .dressing_advice image{
  position: absolute;
  left:45rpx;
  top: 50%;
  margin-top: -45rpx;
}
